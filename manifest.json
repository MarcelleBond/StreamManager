{
	"name": "Stream Manager",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		// {
		// 	"matches": [
		// 		"https://*.spotify.com/*"
		// 	],
		// 	"js": ["spotify.js"]
		// },
		{
			"matches": [
				"https://www.deezer.com/*"
			],
			"js": ["deezer.js"]
		}
	],
	"permissions": [
		"storage",
		"http://localhost:5000/",
		"https://localhost:5001/",
		"tabs",
		"activeTab"
	]
}