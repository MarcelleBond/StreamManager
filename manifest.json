{
	"name": "Stream Manager",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://*.spotify.com/*"
			],
			"js": [
				"spotify.js"
			]
		},
		{
			"matches": [
				"https://www.deezer.com/*"
			],
			"js": [
				"deezer.js"
			]
		}
	],
	"permissions": [
		"storage"
	],
	"host_permissions": [
		"https://night-shadow.eastus.cloudapp.azure.com/*"
	],
	"action": {
		"default_icon": {
			"16": "icon16.png",
			"24": "icon24.png",
			"32": "icon32.png"
		},
		"default_title": "Stream Manager"
	}
}